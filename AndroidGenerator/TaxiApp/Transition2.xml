<?xml version="1.0" encoding="utf-8"?>
<Transition>
        <Button
        	id="@+id/toMap"
        	name_from="main"
        	name_to="map"
        	action = "pushButton" />
        <Button
        	id="@+id/toList"
        	name_from="main"
        	name_to="orders"
        	action = "pushButton" />
        <Button
        	id="@+id/backFromMap"

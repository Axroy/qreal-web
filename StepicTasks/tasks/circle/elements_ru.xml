<?xml version="1.0" encoding="UTF-8" ?>
<Elements>

	<NotVisible>

        <Element name="Линия соединения" type="ControlFlow">
            <Property key="Guard" name="Условие" type="combobox">
                <value key=""></value>
                <Variants>
                    <variant key="false">false</variant>
                    <variant key="iteration">iteration</variant>
                    <variant key="true">true</variant>
                </Variants>
             </Property>
        </Element>

	</NotVisible>

	<Visible>
    
		<Palette>

			<Category name="Алгоритмы">

				<Element name="Начало" type="InitialNode">
					<Image src="images/initialBlock.svg"/>
				</Element>

				<Element name="Конец" type="FinalNode">
					<Image src="images/finalBlock.svg"/>
				</Element>

			</Category>
			<Category name="Действия">

				<Element name="Моторы вперёд" type="TrikV6EnginesForward">
					<Property key="Ports" name="Порты" type="string">
				        <value>M3, M4</value>
					</Property>
					<Property key="Power" name="Мощность (%)" type="string">
				        <value>100</value>
					</Property>
					<Image src="images/enginesForwardBlock.svg"/>
				</Element>

				<Element name="Моторы назад" type="TrikV6EnginesBackward">
					<Property key="Ports" name="Порты" type="string">
				        <value>M3, M4</value>
					</Property>
					<Property key="Power" name="Мощность (%)" type="string">
				        <value>100</value>
					</Property>
					<Image src="images/enginesBackwardBlock.svg"/>
				</Element>

				<Element name="Моторы стоп" type="TrikV6EnginesStop">
					<Property key="Ports" name="Порты" type="string">
				        <value>M1, M2, M3, M4</value>
					</Property>
					<Image src="images/enginesStopBlock.svg"/>
				</Element>

				<Element name="Сбросить показания энкодера" type="TrikV62ClearEncoder">
					<Property key="Ports" name="Порты" type="string">
				        <value>E1, E2, E3, E4</value>
					</Property>
					<Image src="images/nullificationEncoderBlock.svg"/>
				</Element>

			</Category>
			<Category name="Ожидание">

				<Element name="Ждать энкодер" type="TrikV62WaitForEncoder">
					<Property key="Port" name="Порт" type="string">
				        <value>E3</value>
					</Property>
					<Property key="Sign" name="Считанное значение" type="dropdown">
				        <value key="greater">больше</value>
				        <Variants>
				            <variant key="greater">больше</variant>
				            <variant key="less">меньше</variant>
				            <variant key="not greater">не больше</variant>
				            <variant key="not less">не меньше</variant>
				        </Variants>
					</Property>
					<Property key="TachoLimit" name="Предел оборотов" type="string">
				        <value>""</value>
					</Property>
					<Image src="images/waitForEncoderBlock.svg"/>
				</Element>
			</Category>

			<Category name="Рисование">

				<Element name="Опустить маркер" type="MarkerDown">
				    <Property key="Color" name="Цвет" type="dropdown">
				        <value key="blue">синий</value>
				        <Variants>
				            <variant key="black">черный</variant>
				            <variant key="blue">синий</variant>
				            <variant key="cyan">сине-зелёный</variant>
				            <variant key="dark blue">тёмно-синий</variant>
				            <variant key="dark cyan">тёмно-сине-зелёный</variant>
				            <variant key="dark gray">тёмно-серый</variant>
				            <variant key="dark green">тёмно-зелёный</variant>
				            <variant key="dark magenta">тёмно-пурпурный</variant>
				            <variant key="dark red">тёмно-красный</variant>
				            <variant key="dark yellow">тёмно-жёлтый</variant>
				            <variant key="gray">серый</variant>
				            <variant key="green">зелёный</variant>
				            <variant key="light gray">светло-серый</variant>
				            <variant key="magenta">пурпурный</variant>
				            <variant key="red">красный</variant>
				            <variant key="white">белый</variant>
				            <variant key="yellow">жёлтый</variant>
				        </Variants>
				    </Property>
				    <Image src="images/markerDownBlock.svg"/>
				</Element>

				<Element name="Поднять маркер" type="MarkerUp">
				    <Image src="images/markerUpBlock.svg"/>
				</Element>

			</Category>

		</Palette>

	</Visible>

</Elements>

<?xml version="1.0" encoding="UTF-8" ?>
<Elements>

	<NotVisible>

		<Element name="Link" type="ControlFlow">
            <Property key="Guard" name="Guard" type="combobox">
                <value key=""></value>
                <Variants>
                    <variant key="false">false</variant>
                    <variant key="iteration">iteration</variant>
                    <variant key="true">true</variant>
                </Variants>
             </Property>
        </Element>

	</NotVisible>

	<Visible>
    
		<Palette>

			<Category name="Algorithms">

				<Element name="Initial Node" type="InitialNode">
					<Image src="images/initialBlock.svg"/>
				</Element>

				<Element name="Final Node" type="FinalNode">
					<Image src="images/finalBlock.svg"/>
				</Element>

			</Category>
			<Category name="Actions">

				<Element name="Motors Forward" type="TrikV6EnginesForward">
					<Property key="Ports" name="Ports" type="string">
				        <value>M3, M4</value>
					</Property>
					<Property key="Power" name="Power (%)" type="string">
				        <value>100</value>
					</Property>
					<Image src="images/enginesForwardBlock.svg"/>
				</Element>

				<Element name="Motors Backward" type="TrikV6EnginesBackward">
					<Property key="Ports" name="Ports" type="string">
				        <value>M3, M4</value>
					</Property>
					<Property key="Power" name="Power (%)" type="string">
				        <value>100</value>
					</Property>
					<Image src="images/enginesBackwardBlock.svg"/>
				</Element>

				<Element name="Stop Motors" type="TrikV6EnginesStop">
					<Property key="Ports" name="Ports" type="string">
				        <value>M1, M2, M3, M4</value>
					</Property>
					<Image src="images/enginesStopBlock.svg"/>
				</Element>

				<Element name="Clear Encoder" type="TrikV62ClearEncoder">
					<Property key="Ports" name="Ports" type="string">
				        <value>E1, E2, E3, E4</value>
					</Property>
					<Image src="images/nullificationEncoderBlock.svg"/>
				</Element>

			</Category>
			<Category name="Wait">

				<Element name="Wait for Encoder" type="TrikV62WaitForEncoder">
					<Property key="Port" name="Port" type="string">
				        <value>E3</value>
					</Property>
					<Property key="Sign" name="Sign" type="dropdown">
				        <value key="greater">greater</value>
				        <Variants>
				            <variant key="greater">greater</variant>
				            <variant key="less">less</variant>
				            <variant key="not greater">not greater</variant>
				            <variant key="not less">not less</variant>
				        </Variants>
					</Property>
					<Property key="TachoLimit" name="Tacho Limit" type="string">
				        <value>""</value>
					</Property>
					<Image src="images/waitForEncoderBlock.svg"/>
				</Element>
			</Category>

			<Category name="Drawing">

				<Element name="Marker Down" type="MarkerDown">
				    <Property key="Color" name="Color" type="dropdown">
				        <value key="blue">blue</value>
				        <Variants>
				            <variant key="black">black</variant>
				            <variant key="blue">blue</variant>
				            <variant key="cyan">cyan</variant>
				            <variant key="dark blue">dark blue</variant>
				            <variant key="dark cyan">dark cyan</variant>
				            <variant key="dark gray">dark gray</variant>
				            <variant key="dark green">dark green</variant>
				            <variant key="dark magenta">dark magenta</variant>
				            <variant key="dark red">dark red</variant>
				            <variant key="dark yellow">dark yellow</variant>
				            <variant key="gray">gray</variant>
				            <variant key="green">green</variant>
				            <variant key="light gray">light gray</variant>
				            <variant key="magenta">magenta</variant>
				            <variant key="red">red</variant>
				            <variant key="white">white</variant>
				            <variant key="yellow">yellow</variant>
				        </Variants>
				    </Property>
				    <Image src="images/markerDownBlock.svg"/>
				</Element>

				<Element name="Marker Up" type="MarkerUp">
				    <Image src="images/markerUpBlock.svg"/>
				</Element>

			</Category>

		</Palette>

	</Visible>

</Elements>

<?xml version="1.0" encoding="UTF-8" ?>
<Elements>

	<NotVisible>

        <Element name="Линия соединения" type="ControlFlow">
            <Property key="Guard" name="Условие" type="combobox">
                <value key=""></value>
                <Variants>
                    <variant key="false">false</variant>
                    <variant key="iteration">iteration</variant>
                    <variant key="true">true</variant>
                </Variants>
             </Property>
        </Element>

	</NotVisible>

	<Visible>
    
		<Palette>

			<Category name="Алгоритмы">

				<Element name="Начало" type="InitialNode">
					<Image src="images/initialBlock.svg"/>
				</Element>

				<Element name="Конец" type="FinalNode">
					<Image src="images/finalBlock.svg"/>
				</Element>

			</Category>

			<Category name="Ожидание">

				<Element name="Таймер" type="Timer">
					<Property key="Delay" name="Задержка (мс)" type="string">
						<value>1000</value>
					</Property>
					<Image src="images/timerBlock.svg"/>
				</Element>

			</Category>

			<Category name="Рисование">

				<Element name="Цвет кисти" type="TrikSetPainterColor">
				    <Property key="Color" name="Цвет" type="dropdown">
				        <value key="black">черный</value>
				        <Variants>
				            <variant key="black">черный</variant>
				            <variant key="blue">синий</variant>
				            <variant key="cyan">сине-зелёный</variant>
				            <variant key="dark blue">тёмно-синий</variant>
				            <variant key="dark cyan">тёмно-сине-зелёный</variant>
				            <variant key="dark gray">тёмно-серый</variant>
				            <variant key="dark green">тёмно-зелёный</variant>
				            <variant key="dark magenta">тёмно-пурпурный</variant>
				            <variant key="dark red">тёмно-красный</variant>
				            <variant key="dark yellow">тёмно-жёлтый</variant>
				            <variant key="gray">серый</variant>
				            <variant key="green">зелёный</variant>
				            <variant key="light gray">светло-серый</variant>
				            <variant key="magenta">пурпурный</variant>
				            <variant key="red">красный</variant>
				            <variant key="white">белый</variant>
				            <variant key="yellow">жёлтый</variant>
				        </Variants>
				    </Property>
				    <Image src="images/brushColorBlock.svg"/>
				</Element>

				<Element name="Толщина кисти" type="TrikSetPainterWidth">
				    <Property key="Width" name="Ширина" type="string">
				        <value>1</value>
				    </Property>
				    <Image src="images/brushWidthBlock.svg"/>
				</Element>

				<Element name="Нарисовать эллипс" type="TrikDrawEllipse">
				    <Property key="HeightEllipse" name="Высота" type="string">
				        <value>5</value>
				    </Property>

				    <Property key="Redraw" name="Обновить картинку" type="checkbox">
				        <value>true</value>
				    </Property>
				    
				    <Property key="WidthEllipse" name="Ширина" type="string">
				        <value>5</value>
				    </Property>

				    <Property key="XCoordinateEllipse" name="X" type="string">
				        <value>0</value>
				    </Property>
				    
				    <Property key="YCoordinateEllipse" name="Y" type="string">
				        <value>0</value>
				    </Property>
				    <Image src="images/drawCircleBlock.svg"/>
				</Element>
			   
				<Element name="Нарисовать дугу" type="TrikDrawArc">
				    <Property key="HeightArc" name="Высота" type="string">
				        <value>5</value>
				    </Property>
				    
				    <Property key="Redraw" name="Обновить картинку" type="checkbox">
				        <value>true</value>
				    </Property>
				    
				    <Property key="SpanAngle" name="Размах" type="string">
				        <value>20</value>
				    </Property>
		
				    <Property key="StartAngle" name="Начальный угол" type="string">
				        <value>20</value>
				    </Property>

				    <Property key="WidthArc" name="Ширина" type="string">
				        <value>5</value>
				    </Property>
				    
				    <Property key="XCoordinateArc" name="X" type="string">
				        <value>0</value>
				    </Property>

				    <Property key="YCoordinateArc" name="Y" type="string">
				        <value>0</value>
				    </Property>

				    <Image src="images/drawArcBlock.svg"/>
				</Element>

			</Category>

		</Palette>

	</Visible>

</Elements>


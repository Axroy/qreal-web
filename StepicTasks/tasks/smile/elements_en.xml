<?xml version="1.0" encoding="UTF-8" ?>
<Elements>

	<NotVisible>

		<Element name="Link" type="ControlFlow">
            <Property key="Guard" name="Guard" type="combobox">
                <value key=""></value>
                <Variants>
                    <variant key="false">false</variant>
                    <variant key="iteration">iteration</variant>
                    <variant key="true">true</variant>
                </Variants>
             </Property>
        </Element>

	</NotVisible>

	<Visible>
    
		<Palette>

			<Category name="Algorithms">

				<Element name="Initial Node" type="InitialNode">
				    <Image src="images/initialBlock.svg"/>
				</Element>

				<Element name="Final Node" type="FinalNode">
				    <Image src="images/finalBlock.svg"/>
				</Element>

			</Category>

			<Category name="Wait">

				<Element name="Timer" type="Timer">
				    <Property key="Delay" name="Delay (ms)" type="string">
				        <value>1000</value>
				    </Property>
				    <Image src="images/timerBlock.svg"/>
				</Element>

			</Category>

			<Category name="Drawing">

				<Element name="Painter Color" type="TrikSetPainterColor">
				    <Property key="Color" name="Color" type="dropdown">
				        <value key="black">black</value>
				        <Variants>
				            <variant key="black">black</variant>
				            <variant key="blue">blue</variant>
				            <variant key="cyan">cyan</variant>
				            <variant key="dark blue">dark blue</variant>
				            <variant key="dark cyan">dark cyan</variant>
				            <variant key="dark gray">dark gray</variant>
				            <variant key="dark green">dark green</variant>
				            <variant key="dark magenta">dark magenta</variant>
				            <variant key="dark red">dark red</variant>
				            <variant key="dark yellow">dark yellow</variant>
				            <variant key="gray">gray</variant>
				            <variant key="green">green</variant>
				            <variant key="light gray">light gray</variant>
				            <variant key="magenta">magenta</variant>
				            <variant key="red">red</variant>
				            <variant key="white">white</variant>
				            <variant key="yellow">yellow</variant>
				        </Variants>
				    </Property>
				    <Image src="images/brushColorBlock.svg"/>
				</Element>

				<Element name="Painter Width" type="TrikSetPainterWidth">
				    <Property key="Width" name="Width" type="string">
				        <value>1</value>
				    </Property>
				    <Image src="images/brushWidthBlock.svg"/>
				</Element>

				<Element name="Draw Ellipse" type="TrikDrawEllipse">
				    <Property key="HeightEllipse" name="Height" type="string">
				        <value>5</value>
				    </Property>

				    <Property key="Redraw" name="Redraw" type="checkbox">
				        <value>true</value>
				    </Property>
				    
				    <Property key="WidthEllipse" name="Width" type="string">
				        <value>5</value>
				    </Property>

				    <Property key="XCoordinateEllipse" name="X" type="string">
				        <value>0</value>
				    </Property>
				    
				    <Property key="YCoordinateEllipse" name="Y" type="string">
				        <value>0</value>
				    </Property>
				    <Image src="images/drawCircleBlock.svg"/>
				</Element>
			   
				<Element name="Draw Arc" type="TrikDrawArc">
				    <Property key="HeightArc" name="Height" type="string">
				        <value>5</value>
				    </Property>
				    
				    <Property key="Redraw" name="Redraw" type="checkbox">
				        <value>true</value>
				    </Property>
				    
				    <Property key="SpanAngle" name="SpanAngle" type="string">
				        <value>20</value>
				    </Property>
		
				    <Property key="StartAngle" name="StartAngle" type="string">
				        <value>20</value>
				    </Property>

				    <Property key="WidthArc" name="Width" type="string">
				        <value>5</value>
				    </Property>
				    
				    <Property key="XCoordinateArc" name="X" type="string">
				        <value>0</value>
				    </Property>

				    <Property key="YCoordinateArc" name="Y" type="string">
				        <value>0</value>
				    </Property>

				    <Image src="images/drawArcBlock.svg"/>
				</Element>

			</Category>

		</Palette>

	</Visible>

</Elements>


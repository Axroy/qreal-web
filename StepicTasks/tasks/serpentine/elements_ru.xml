<?xml version="1.0" encoding="UTF-8" ?>
<Elements>

	<NotVisible>

        <Element name="Линия соединения" type="ControlFlow">
            <Property key="Guard" name="Условие" type="combobox">
                <value></value>
                <Variants>
                    <variant>false</variant>
                    <variant>iteration</variant>
                    <variant>true</variant>
                </Variants>
             </Property>
        </Element>

	</NotVisible>

	<Visible>
    
		<Palette>

			<Category name="Алгоритмы">

				<Element name="Начало" type="InitialNode">
					<Image src="images/initialBlock.svg"/>
				</Element>

				<Element name="Конец" type="FinalNode">
					<Image src="images/finalBlock.svg"/>
				</Element>

				<Element name="Функция" type="Function">
					<Property key="Body" name="Тело функции" type="string">
						<value></value>
					</Property>
					<Image src="images/functionBlock.svg"/>
				</Element>

			</Category>
			<Category name="Действия">

				<Element name="Моторы вперёд" type="TrikV6EnginesForward">
					<Property key="Ports" name="Порты" type="string">
				        <value>M3, M4</value>
					</Property>
					<Property key="Power" name="Мощность (%)" type="string">
				        <value>100</value>
					</Property>
					<Image src="images/enginesForwardBlock.svg"/>
				</Element>

				<Element name="Моторы назад" type="TrikV6EnginesBackward">
					<Property key="Ports" name="Порты" type="string">
				        <value>M3, M4</value>
					</Property>
					<Property key="Power" name="Мощность (%)" type="string">
				        <value>100</value>
					</Property>
					<Image src="images/enginesBackwardBlock.svg"/>
				</Element>

				<Element name="Моторы стоп" type="TrikV6EnginesStop">
					<Property key="Ports" name="Порты" type="string">
				        <value>M1, M2, M3, M4</value>
					</Property>
					<Image src="images/enginesStopBlock.svg"/>
				</Element>

				<Element name="Сбросить показания энкодера" type="TrikV62ClearEncoder">
					<Property key="Ports" name="Порты" type="string">
				        <value>E1, E2, E3, E4</value>
					</Property>
					<Image src="images/nullificationEncoderBlock.svg"/>
				</Element>

			</Category>
			<Category name="Ожидание">

				<Element name="Таймер" type="Timer">
					<Property key="Delay" name="Задержка (мс)" type="string">
						<value>1000</value>
					</Property>
					<Image src="images/timerBlock.svg"/>
				</Element>

				<Element name="Ждать энкодер" type="TrikV62WaitForEncoder">
					<Property key="Port" name="Порт" type="string">
				        <value>E3</value>
					</Property>
					<Property key="Sign" name="Считанное значение" type="dropdown">
				        <value>greater</value>
				        <Variants>
				            <variant>greater</variant>
				            <variant>less</variant>
				            <variant>not greater</variant>
				            <variant>not less</variant>
				        </Variants>
					</Property>
					<Property key="TachoLimit" name="Предел оборотов" type="string">
				        <value>""</value>
					</Property>
					<Image src="images/waitForEncoderBlock.svg"/>
				</Element>
			</Category>

		</Palette>

	</Visible>

</Elements>
